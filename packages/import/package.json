{
  "name": "@batchactions/import",
  "version": "0.0.3",
  "description": "Import facade for @batchactions/core with schema validation, parsers, and template generation",
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "peerDependencies": {
    "@batchactions/core": ">=0.0.1"
  },
  "dependencies": {
    "papaparse": "^5.5.0"
  },
  "devDependencies": {
    "@batchactions/core": "*",
    "@types/papaparse": "^5.3.14",
    "typescript": "^5.7.3",
    "vitest": "^4.0.0",
    "tsup": "^8.5.1"
  },
  "keywords": [
    "bulk-import",
    "csv",
    "json",
    "xml",
    "schema-validation",
    "batch-processing"
  ],
  "author": "Victor Garcia <vgpastor@ingenierosweb.co>",
  "homepage": "https://github.com/vgpastor/batchactions/tree/main/packages/import#readme",
  "bugs": {
    "url": "https://github.com/vgpastor/batchactions/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vgpastor/batchactions",
    "directory": "packages/import"
  },
  "license": "MIT"
}
