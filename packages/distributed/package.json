{
  "name": "@bulkimport/distributed",
  "version": "0.1.0",
  "description": "Distributed parallel batch processing for @bulkimport/core — fan-out N workers to process batches concurrently",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ tests/"
  },
  "keywords": [
    "bulkimport",
    "distributed",
    "parallel",
    "batch-processing",
    "serverless",
    "lambda",
    "worker"
  ],
  "license": "MIT",
  "author": "Víctor Garcia <vgpastor@ingenierosweb.co>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vgpastor/bulkimport",
    "directory": "packages/distributed"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "@bulkimport/core": ">=0.4.0"
  },
  "devDependencies": {
    "@bulkimport/core": "file:../../",
    "@types/node": "^22.13.4",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  }
}
