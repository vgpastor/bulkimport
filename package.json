{
  "name": "@bulkimport/core",
  "version": "0.1.0",
  "description": "Backend-agnostic bulk data import library with schema validation, batch processing, and state management",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run typecheck && npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "bulk",
    "import",
    "csv",
    "json",
    "xml",
    "batch",
    "validation",
    "schema",
    "data-import",
    "etl",
    "stream",
    "parser"
  ],
  "license": "MIT",
  "author": "VÃ­ctor Garcia <vgpastor@ingenierosweb.co>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vgpastor/bulkimport"
  },
  "engines": {
    "node": ">=16.7.0"
  },
  "sideEffects": false,
  "dependencies": {
    "papaparse": "^5.5.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "@types/papaparse": "^5.3.15",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitest/coverage-v8": "^3.0.6",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.5.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  }
}
